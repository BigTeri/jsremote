{
  "version": "0.2.0",
  "configurations": [{
    "name": "Launch Server",
    "type": "node2",
    "request": "launch",
    "program": "${workspaceRoot}/server/cli.js",
    "stopOnEntry": false,
    "cwd": "${workspaceRoot}",
    "preLaunchTask": "dev",
    "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/babel-node",
    "runtimeArgs": [
      "--nolazy"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "console": "internalConsole",
    "port": 5858
  }]
}
