{
  "name": "jsremote",
  "version": "0.3.0",
  "description": "NodeJS Desktop Remote using W3C Pointer Lock",
  "main": "server/index.js",
  "scripts": {
    "start": "babel-node server/cli.js",
    "debug-cli": "npm run build && node ./dist/cli.js -p 4000",
    "build": "babel server --out-dir dist",
    "postinstall": "jspm i --production"
  },
  "bin": {
    "jsRemote": "./bin/index.js"
  },
  "author": "ardean",
  "license": "MIT",
  "keywords": [
    "desktop",
    "html5-pointer-lock",
    "javascript",
    "jspointerlock",
    "nodejs",
    "pointerlock",
    "remote",
    "remote-desktop",
    "remote-control"
  ],
  "repository": "https://github.com/ardean/jsRemote",
  "bugs": "https://github.com/ardean/jsRemote/issues",
  "dependencies": {
    "commander": "^2.9.0",
    "express": "^4.14.0",
    "jspm": "^0.16.47",
    "robotjs": "^0.4.5",
    "socket.io": "^1.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-node6": "^11.0.0"
  },
  "jspm": {
    "directories": {
      "baseURL": "client"
    },
    "dependencies": {
      "events": "github:jspm/nodelibs-events@^0.1.1",
      "jquery": "npm:jquery@^3.1.1",
      "jspointerlock": "github:ardean/jsPointerLock@^0.3.3"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
