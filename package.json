{
  "name": "jsremote",
  "version": "0.5.6",
  "description": "NodeJS Desktop Remote using W3C Pointer Lock & Events",
  "main": "dist/server/index.js",
  "scripts": {
    "start": "gulp dev && babel-node server/cli.js",
    "build": "yarn cleanup && yarn build-server && yarn build-client",
    "build-server": "babel server --out-dir dist/server",
    "build-client": "yarn build-js && gulp build-client && yarn build-css",
    "build-js": "jspm build src/index.js dist/client/index.js --minify --skip-source-maps",
    "build-css": "cpy client/css/**/*.css dist/client/css",
    "cleanup": "del-cli dist",
    "test": "yarn install && jspm i && yarn run build"
  },
  "bin": {
    "jsremote": "./bin/index.js",
    "jsRemote": "./bin/index.js"
  },
  "author": "ardean",
  "license": "MIT",
  "keywords": [
    "desktop",
    "html5-pointer-lock",
    "javascript",
    "jsremote",
    "jspointerlock",
    "nodejs",
    "pointerlock",
    "remote",
    "remote-desktop",
    "remote-control",
    "pointer-events",
    "touch-support",
    "touch"
  ],
  "repository": "https://github.com/ardean/jsRemote",
  "bugs": "https://github.com/ardean/jsRemote/issues",
  "dependencies": {
    "chalk": "^2.3.0",
    "commander": "^2.9.0",
    "express": "^4.14.0",
    "robotjs": "^0.5.0",
    "socket.io": "^2.0.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-node6": "^11.0.0",
    "cpy-cli": "^1.0.1",
    "del-cli": "^1.1.0",
    "gulp": "^3.9.1",
    "gulp-inject": "^4.2.0",
    "jspm": "^0.17.0-beta.38",
    "require-dir": "^0.3.1",
    "yarn": "^1.3.2"
  },
  "jspm": {
    "name": "jsremote",
    "main": "client/index.js",
    "directories": {
      "baseURL": "client"
    },
    "dependencies": {
      "jspointerlock": "github:ardean/jsPointerLock@^1.0.0",
      "events": "npm:jspm-nodelibs-events@^0.2.0",
      "jquery": "npm:jquery@^3.1.1"
    },
    "devDependencies": {
      "plugin-babel": "npm:systemjs-plugin-babel@^0.0.20"
    },
    "peerDependencies": {
      "assert": "npm:jspm-nodelibs-assert@^0.2.0",
      "process": "npm:jspm-nodelibs-process@^0.2.0",
      "util": "npm:jspm-nodelibs-util@^0.2.0"
    },
    "overrides": {
      "npm:jquery@3.1.1": {
        "format": "amd"
      }
    }
  }
}