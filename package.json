{
  "name": "jsremote",
  "version": "0.2.0",
  "description": "NodeJS desktop remote using HTML5 Pointerlock",
  "main": "server/index.js",
  "scripts": {
    "start": "babel-node server --presets es2015-node",
    "debug-cli": "npm run build && node ./dist/cli.js -p 4000",
    "build": "babel server --out-dir dist --presets es2015-node",
    "postinstall": "jspm i --production"
  },
  "bin": {
    "jsremote": "./bin/index.js"
  },
  "author": "BigTeri",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BigTeri/jsremote.git"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "express": "^4.14.0",
    "jspm": "^0.16.47",
    "robotjs": "^0.4.5",
    "socket.io": "^1.5.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2015-node": "^6.1.1"
  },
  "jspm": {
    "directories": {
      "baseURL": "client"
    },
    "dependencies": {
      "events": "github:jspm/nodelibs-events@^0.1.1",
      "jquery": "npm:jquery@^3.1.1",
      "jspointerlock": "npm:jspointerlock@^0.1.3"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4"
    }
  }
}
